/*
 * This code emulates the noise generated by most 315/434MHz
 * receivers, you can use it to tune the servocto's noise
 * filter.
 * 
 * Written by Daniel Sitrakiniaina Razafimaharavo
 */
void setup() {

  pinMode(9, OUTPUT);
}

// the loop function runs over and over again forever
void loop() {
  digitalWrite(9, HIGH);
  delayMicroseconds(random(1000,5000)); 
  digitalWrite(9, LOW); 
  delayMicroseconds(random(1100,4900));       
}
